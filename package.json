{
  "name": "capacitor-stripe-terminal",
  "version": "0.0.1",
  "description": "Capacitor plugin for Stripe Terminal (credit card readers).",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "docs": "typedoc --mode file --excludeExternals --excludeNotExported --out docs src"
  },
  "author": "eventOne Labs <opensource@event1.io>",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@capacitor/android": "latest",
    "@capacitor/ios": "latest",
    "semantic-release": "^16.0.0-beta.26",
    "typedoc": "^0.15.0",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorStripeTerminal.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "stripe",
    "stripe-terminal"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eventOneHQ/capacitor-stripe-terminal"
  },
  "bugs": {
    "url": "https://github.com/eventOneHQ/capacitor-stripe-terminal/issues"
  },
  "release": {
    "branches": [
      {
        "name": "next"
      },
      {
        "name": "beta",
        "channel": "beta",
        "prerelease": true
      }
    ]
  }
}
